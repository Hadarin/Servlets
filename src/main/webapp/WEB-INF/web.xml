  </servlet>

  <servlet-mapping>
    <servlet-name>PostServlet2</servlet-name>
    <url-pattern>/do2</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>PostServlet1</servlet-name>
    <servlet-class>PostServlet1</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>PostServlet1</servlet-name>
    <url-pattern>/do1</url-pattern>
  </servlet-mapping>

</web-app>
